	<body>
		<div class="box-container">
			<div class="top-nav">
				<div class=left-menu>
					<h4 class="title">Dilab</h4>
					<h4 class="opt">Releases</h4>
					<h4 class="opt">Projects</h4>
					<h5 class="opt">Groups</h5>
				</div>
				<div class=right-menu>
					<div class=search-container>
						<input class=search-field maxlength="40" type=text placeholder="Search content..">
						<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
							<path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
						</svg>
					</div>
					<% if (connected==false) {%>
						<button class="button loginButton">Login</button>
					<% } else { %>
						<div class="notificationsButton">
							<i class="bi bi-bell"></i>
						</div>
						<div class=notificationsMenu>
							<h2>Notifications</h1>
							<div class=notificationsList>
								<div class="notification">
									<div class=icon>
										<i class="bi bi-person-plus-fill"></i>
									</div>
									<div class=text>
										<strong>Jaja</strong> wants to join your group <strong>Croum Crawm</strong>
									</div>
									<div class=options>
										<div class=accept>
											<i class="bi bi-check2"></i>
										</div>
										<div class=deny>
											<i class="bi bi-x"></i>
										</div>
									</div>
								</div>
								<hr />
								<div class="notification">
									<div class=icon>
										<i class="bi bi-people-fill"></i>
									</div>
									<div class=text>
										<strong>Jojo</strong> texted you
									</div>
									<div class=options>
										<div class=confirm>
											<i class="bi bi-arrow-right"></i>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class=userProfileCircleButton><img class=userPPImg src=/dilab/user/person-circle.svg /></div>
						<div class=userMainMenu></a><div class=menuPP><img class=menuPPImg src=/dilab/user/person-circle.svg /></div><div class=infoText>Welcome,<br /><span class=nameAndSurname></span> !</div><div class="settingsBtn aButton"><i class="bi bi-gear-fill"></i>Settings</div><div class="aButton disconnectBtn"><i class="bi bi-power"></i>Log Out</div></div>
					<%}%>
				</div>
				<div class=progress-load></div>
			</div>
			<div class=main-content>
				<div class="kabobloader">
					<div class="bounce1"></div>
					<div class="bounce2"></div>
					<div class="bounce3"></div>
				  </div>
			</div>
			<div class=player>
				<div class="musicInfos">
					<div title="Display lyrics (if available)" class="cover">
